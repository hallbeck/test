<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="desktop" verbose="1"  parallel="methods" thread-count="1">
    <test name="NIsmokeDesktop">
        <parameter name="device" value='desktop'/>
        <parameter name="prod" value='no'/>
        <classes>
            <class name="Regression.Rebates.Rebates_Frequency55MultiD_48580"/>
            <class name="Regression.Rebates.Rebates_Frequency55MultiN_48580"/>
        </classes>
    </test>
    <test name="RIsmokeDesktop">
        <parameter name="device" value='desktop'/>
        <parameter name="prod" value='no'/>
        <classes>
            <class name="SMOKE.SmokeRIAcuvue2AccessoryCanadaTest"/>
            <class name="SMOKE.SmokeRIColorToricAccessoryTest"/>
            <class name="SMOKE.SmokeRIProgressiveAccessoryInternationalTest"/>
        </classes>
    </test>
</suite>